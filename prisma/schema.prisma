// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hotel{
id                Int       @id @default(autoincrement())
namaHotel         String
deskripsiHotel    String
fotohotel         String
fotohotel2        String
fotohotel3        String
fotohotel4        String
}